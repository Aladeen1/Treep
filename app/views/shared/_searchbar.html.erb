<div class="choix3">
    
  </div>

  <div class="border-form">
    <i class="fas fa-plane-departure"></i>
    <i class="fas fa-plane-arrival"></i>
    <form class="search" method="get" action="http://localhost:3000/search">
      <ul>
        <li class="allez-retour">
          <select name="flight_type" id="flight_type">
            <option value="oneway">Aller simple</option>
            <option selected="selected" value="round">Aller retour</option>
          </select>
          <li><i class="fas fa-arrow-down"></i></li>
        </li>

        <li class="passager">
          <input type="number" value="1" min="1" max="9" name="passengers" id="passengers">
          <li><i class="fas fa-arrow-down"></i></li>
        </li>
      </ul>
      <input  type="text" name="depart" class="search__field" id="departLoc__search" placeholder="Paris (PAR)" list="airports__depart">
      <input type="text" name="return" class="search__field" id="returnLoc__search" placeholder="Ã€ ? " list="airports__return">
      <input type="text" name="from" class="search__field" id="departDate__search" placeholder="Date allez">
      <input type="text" name="to" class="search__field" id="returnDate__search" placeholder="Date retour">
      <button class="btn search__btn">
          <span>Rechercher</span>
      </button>
    </form>
  </div>

